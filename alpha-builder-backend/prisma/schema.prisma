generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum UserType {
  BINANCE_SHADOW
  NATIVE
}

model User {
  id               String    @id @default(uuid())
  type             UserType
  emailCommitment  String?   @db.VarChar(66)
  binanceWallet    String?   @db.VarChar(255)
  aaWalletAddress  String    @unique @db.VarChar(255)
  ownerAddress     String    @db.VarChar(255)
  salt             String    @db.VarChar(255)
  createdAt        DateTime  @default(now())
  updatedAt        DateTime  @updatedAt
}
